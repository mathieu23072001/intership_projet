{% extends 'base.html.twig' %}

{% block title %}Prefecture{% endblock %}

{% block body %}
    <h1>Prefecture</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prefecture.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ prefecture.libelle }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ prefecture.description }}</td>
            </tr>
            <tr>
                <th>Region</th>
                <td>{{ prefecture.region }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('prefecture_index') }}">back to list</a>

    <a href="{{ path('prefecture_edit', {'id': prefecture.id}) }}">edit</a>

    {{ include('prefecture/_delete_form.html.twig') }}
{% endblock %}
