{% extends "baseSupAd.html.twig" %}

{% block stylesheets %} {{  parent() }} {% endblock %}

{% block title %} {{  parent() }} {% endblock %}


{% block body %}



<div class="row bg-title">
 
                <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                    <h4 class="page-title">Liste des stagiaires </h4>
                </div>
                <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12"> 
                    <o class="breadcrumb">
                        <li>Entrprise </li>
                        <li class="active"> Liste des stagiaires</li>
                    </o>
                </div>
                
                <!-- /.col-lg-12 -->
            </div>


<div class="row">
                    <div class="col-lg-12">
                        <div class="white-box">
                            <h3 class="box-title">Etudiants candidats</h3>
                            <p class="text-muted">Consulez le profil ou cv de l'etudiant candidat</p>
                            <table class="table table-striped table-bordered" id="">
                                <thead>
                                    <tr>
                                        <th>Photo </th>
                                        <th>Theme </th>
                                        
                                        <th>Nom </th>
                                        <th>Prenom </th>
                                        
                                        
                                        <th>Specialite</th>
                                         

                                        
                                        
                                        
                                        <th>Niveau</th>
                                        
                                        
                                        <th>Sexe</th>
                                        
                                        <th>Adresse</th>
                                         <th> Telephone</th>
                                        
                                        
                                        
                                        
                                    </tr>
                                </thead>
                                <tbody>
                                {% for candidature in candidature %}
                            
                                
                                    <tr id="1" class="gradeX">
                                        <td><img src="{{asset('images/etudiants/' ~ candidature.etudiant.image)}}" alt="" width="80" height="80"></td>
                                         <td>{{candidature.offre.titre}}</td>
                                        <td>{{candidature.etudiant.nom}}</td>
                                        <td>{{candidature.etudiant.prenoms}} </td>
                                         
                                        
                                         <td>{{candidature.etudiant.specialite.nom}} </td>
                                        
                                        
                                        
                                          <td>{{candidature.etudiant.niveau.nom}}</td>
                                        
                                        
                                         <td>{{candidature.etudiant.sexe}}</td>
                                        
                                     <td class="center">{{candidature.etudiant.adresse}}</td>
                                       <td class="center">{{candidature.etudiant.telephone}}</td>
                                       
                                     
                                        
                                
                                       
                                        
                                    </tr>
                                   
                                   {% endfor %}
                                </tbody>
                                <tfoot>
                                    <tr>
                                       <th>Photo </th>
                                       <th>Thme </th>
                                        <th>Nom </th>
                                        <th>Prenom </th>
                                        
                                        
                                        <th>Specialite</th>
                                         

                                        
                                        
                                        
                                        <th>Niveau</th>
                                        
                                        
                                        <th>Sexe</th>
                                        
                                        <th>Adresse</th>
                                         <th> Telephone</th>
                                        
                                        
                                        
                                       
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
                    </div>
                </div>
             <!-- /.row -->


                     </div>
                  </div>
               </div>
               <!-- .row -->
					
					






{% endblock %}

